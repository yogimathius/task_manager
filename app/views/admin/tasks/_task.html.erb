<div id="<%= dom_id task %>">
  <p>
    <strong>Title:</strong>
    <%= task.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= task.description %>
  </p>

  <p>
    <strong>Done:</strong>
    <%= task.done %>
  </p>

  <p>
    <strong>Completed at:</strong>
    <%= task.completed_at %>
  </p>

</div>
