<p style="color: green"><%= notice %></p>

<h1>Subtasks</h1>

<div id="subtasks">
  <% @subtasks.each do |subtask| %>
    <%= render subtask %>
    <p>
      <%= link_to "Show this subtask", admin_subtask_path(subtask) %>
    </p>
  <% end %>
</div>

<%= link_to "New subtask", new_admin_subtask_path %>
